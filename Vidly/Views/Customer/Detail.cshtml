@model Vidly.Models.Customer
@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Membership Type: @Model.MembershipTypes.Name </li>
    @if (@Model.Birthday != null)
    {
        <li>Birthday: @Model.Birthday.Value.ToShortDateString()</li>
    }
</ul>

